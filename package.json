{
  "name": "react-serverless-auth-example",
  "version": "1.0.0",
  "private": false,
  "license": "MIT",
  "description": "An example implementation of react-serverless-auth React Components",
  "keywords": [
    "react",
    "serverless",
    "auth",
    "example",
    "amazon",
    "aws",
    "cognito"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/stanchino/react-serverless-auth/example"
  },
  "dependencies": {
    "history": "^4.7.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "next",
    "react-scripts": "^1.1.0",
    "react-serverless-auth": "https://github.com/stanchino/react-serverless-auth.git#development",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "build": "react-scripts build"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "coveralls": "^3.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^20.0.4",
    "jest-enzyme": "^4.2.0",
    "react-test-renderer": "^16.2.0",
    "redux-mock-store": "^1.5.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/shared-examples.{js,jsx}",
      "!src/*.js",
      "!<rootDir>/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "coverageReporters": [
      "text",
      "lcov"
    ]
  }
}
